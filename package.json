{
  "name": "generator-lionbyte",
  "version": "2.4.0",
  "description": "Generate a JavaScript project",
  "scripts": {
    "format": "npm run prettier && npm run lint -- --fix",
    "lint": "eslint --ignore-path .gitignore .",
    "prettier": "prettier --ignore-path .gitignore --write \"**/*.+(js|json|yml|yaml|css|less|ts|md|graphql|mdx|html)\"",
    "type": "tsc --pretty",
    "test": "npm run type && jest",
    "semantic-release": "semantic-release"
  },
  "license": "MIT",
  "main": "generators/index.js",
  "author": "Mark Hernandez <mark@lion-byte.com> (https://www.lion-byte.com)",
  "repository": {
    "type": "git",
    "url": "https://github.com/MarkH817/generator-lionbyte.git"
  },
  "files": [
    "generators"
  ],
  "keywords": [
    "generator",
    "scaffold",
    "project",
    "nodejs",
    "yeoman-generator"
  ],
  "devDependencies": {
    "@lionbyte/eslint-config": "^1.6.0",
    "@semantic-release/changelog": "^3.0.6",
    "@semantic-release/git": "^8.0.0",
    "@types/jest": "^24.9.0",
    "@types/node": "^13.1.8",
    "@types/yeoman-generator": "^3.1.4",
    "@types/yeoman-test": "^2.0.3",
    "@types/yosay": "0.0.29",
    "eslint": "^6.8.0",
    "husky": "^4.2.0",
    "jest": "^25.1.0",
    "jest-runner-eslint": "^0.7.5",
    "lint-staged": "^10.0.1",
    "prettier": "^1.19.1",
    "semantic-release": "^16.0.2",
    "typescript": "^3.7.5",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^2.1.0"
  },
  "dependencies": {
    "chalk": "^3.0.0",
    "yeoman-generator": "^4.4.0",
    "yosay": "^2.0.2"
  }
}
