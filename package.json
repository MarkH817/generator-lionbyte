{
  "name": "generator-lionbyte",
  "version": "2.8.0",
  "description": "Generate a JavaScript project",
  "scripts": {
    "format": "npm run prettier && npm run lint -- --fix",
    "lint": "eslint --ignore-path .gitignore .",
    "prettier": "prettier --ignore-path .gitignore --write \"**/*.+(js|json|yml|yaml|css|less|ts|md|graphql|mdx|html)\"",
    "type": "tsc --pretty",
    "test": "npm run type && jest",
    "semantic-release": "semantic-release"
  },
  "license": "MIT",
  "main": "generators/index.js",
  "author": "Mark Hernandez <mark@lion-byte.com> (https://www.lion-byte.com)",
  "repository": {
    "type": "git",
    "url": "https://github.com/MarkH817/generator-lionbyte.git"
  },
  "files": [
    "generators"
  ],
  "keywords": [
    "generator",
    "scaffold",
    "project",
    "nodejs",
    "yeoman-generator"
  ],
  "devDependencies": {
    "@lionbyte/eslint-config": "^1.7.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.11.1",
    "@types/yeoman-assert": "^3.1.1",
    "@types/yeoman-generator": "^3.1.4",
    "@types/yeoman-test": "^2.0.3",
    "@types/yosay": "0.0.29",
    "eslint": "^6.8.0",
    "husky": "^4.2.5",
    "jest": "^25.3.0",
    "jest-runner-eslint": "^0.7.6",
    "lint-staged": "^10.1.3",
    "prettier": "^2.0.4",
    "semantic-release": "^17.0.4",
    "typescript": "^3.8.3",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^2.4.1"
  },
  "dependencies": {
    "chalk": "^3.0.0",
    "yeoman-generator": "^4.8.0",
    "yosay": "^2.0.2"
  }
}
